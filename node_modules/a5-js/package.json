{
  "name": "a5-js",
  "description": "A5 - Global Pentagonal Geospatial Index",
  "version": "0.5.0",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixpalmer/a5.git"
  },
  "main": "./dist/a5.cjs",
  "module": "./dist/a5.js",
  "types": "./dist/a5.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/a5.d.ts",
        "default": "./dist/a5.cjs"
      },
      "default": {
        "types": "./dist/a5.d.ts",
        "default": "./dist/a5.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "tsup --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsup",
    "generate-fixtures": "yarn build && node scripts/generate-fixtures.cjs"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "node-fetch": "^3.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0",
    "vitest": "^1.3.1"
  },
  "volta": {
    "node": "22.13.1",
    "yarn": "4.6.0"
  },
  "packageManager": "yarn@4.6.0"
}
